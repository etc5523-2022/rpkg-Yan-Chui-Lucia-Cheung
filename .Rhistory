max_pribreed <- function(primary_breed) {
zipcode <- seattle_pets %>%
select(zip_code) %>%
distinct(zip_code)
zipcode <- unique(zipcode$zip_code)
max_pribreed <- seattle_pets %>%
filter(primary_breed == primary_breed) %>%
group_by(zip_code) %>%
summarize(count = n()) %>%
arrange(-count) %>%
head(5)
print(max_pribreed)
}
